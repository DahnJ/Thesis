@Article{Zessin2008,
author="Zessin, H.",
title="Point processes in general position",
journal="Journal of Contemporary Mathematical Analysis",
year="2008",
month="Feb",
day="01",
volume="43",
number="1",
pages="59--65",
abstract="The aim of this note is to introduce for point processes in Rdthe notions general position and reinforced general position, and to characterize these processes. As a consequence we show that Poisson processes P$\rho$with an infinite intensity measures $\rho$ are in general position iff $\rho$ is diffuse in the sense that any affine subspace of dimension d - 1 is a $\rho$-nullset. Moreover, P$\rho$is in reinforced general position iff in addition any (d - 1)-sphere is a $\rho$-nullset.",
issn="1934-9416",
doi="10.3103/s11957-008-1005-x",
url="https://doi.org/10.3103/s11957-008-1005-x"
}



